test_show_df:
  -
    id: well
    df:
      -
        well: 'A1'
        x: 1
      -
        well: 'A2'
        x: 2
    expected: one_param.png
  -
    id: well0
    df:
      -
        well0: 'A01'
        x: 1
      -
        well0: 'A02'
        x: 2
    expected: one_param.png
  -
    id: row-col
    df:
      -
        row: 'A'
        col: '1'
        x: 1
      -
        row: 'A'
        col: '2'
        x: 2
    expected: one_param.png
  -
    id: row-i-col-j
    df:
      -
        row_i: 0
        col_j: 0
        x: 1
      -
        row_i: 0
        col_j: 1
        x: 2
    expected: one_param.png
  -
    id: err-no-loc
    df:
      -
        x: 1
      -
        x: 2
    error:
      type: LayoutError
      message:
        - Can't find well locations
        - Data frame must have 1-2 of the following columns: 'well', 'well0', 'row', 'col', 'row_i', 'col_j'
        - Columns found: 'x'


test_pick_params:
  -
    id: pick-ab
    df:
      -
        a: 1
        b: 2
      -
        a: 3
        b: 4

    params: []
    expected:
      [a,b]
  -
    id: pick-a
    df:
      -
        a: 1
        b: 2
      -
        a: 3
        b: 2

    params: []
    expected:
      [a]
  -
    id: err-empty
    df:
      []
    params: []
    error:
      type: LayoutError
      message: No experimental parameters found
  -
    id: err-degenerate
    df:
      -
        a: 1
        b: 2
      -
        a: 1
        b: 2

    params: []
    error:
      type: UsageError
      message: Found only degenerate parameters (i.e. with the same value in every well): 'a', 'b'
  -
    id: choose-a-str
    df:
      -
        a: 1
        b: 2
      -
        a: 1
        b: 2

    params: 'a'
    expected:
      - a
  -
    id: choose-a-list
    df:
      -
        a: 1
        b: 2
      -
        a: 1
        b: 2

    params: ['a']
    expected:
      - a
  -
    id: choose-ab
    df:
      -
        a: 1
        b: 2
      -
        a: 1
        b: 2

    params: ['a', 'b']
    expected:
      [a, b]
  -
    id: err-not-found
    df:
      -
        a: 1
        b: 2
      -
        a: 1
        b: 2

    params: 'c'
    error:
      type: UsageError
      message:
        - No such parameter: 'c'
        - Did you mean: 'a', 'b'
  -
    id: err-not-found-plural
    df:
      -
        a: 1
        b: 2
      -
        a: 1
        b: 2

    params: ['c', 'd']
    error:
      type: UsageError
      message:
        - No such parameters: 'c', 'd'
        - Did you mean: 'a', 'b'
