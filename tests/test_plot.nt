      > y = 'a'
      >
      > [well.A2]
      > x = 2
      > y = 'b'
    params:
      [x, y]
    expected: user_params_xy.png
  -
    id: sort-numbers
    layout:
      > [well]
      > A1.x = 6
      > A2.x = 5
      > A3.x = 4
      > A4.x = 1
      > A5.x = 2
      > A6.x = 3
    expected: sort_numbers.png
  -
    id: sort-dates
    layout:
      > [well]
      > A1.x = 2018-07-06
      > A2.x = 2018-07-05
      > A3.x = 2018-07-04
      > A4.x = 2018-07-01
      > A5.x = 2018-07-02
      > A6.x = 2018-07-03
    expected: sort_dates.png
  -
    id: sort-strings
    layout:
      > [well]
      > A1.x = 'f'
      > A2.x = 'e'
      > A3.x = 'd'
      > A4.x = 'a'
      > A5.x = 'b'
      > A6.x = 'c'
    expected: sort_strings.png
  -
    id: sort-plate
    layout:
      > [plate.A.well]
      > A1.x = 1
      > A2.x = 2
      >
      > [plate.B.well]
      > A1.x = 3
      > A2.x = 4
    expected: sort_plate.png
  -
    id: sort-concave
    layout:
      > # Test for a bug where the rows and columns are sorted independently,
      > # such that the `x=2` wells appear to occupy (0, 0).
      > [well]
      > A2.x = 1
      > A3.x = 2
      > B1.x = 2
    expected: sort_concave.png
  -
    id: nan-first
    layout:
      > [well.A1]
      > x = 'a'
      >
      > [well.A2]
      > x = 'a'
      > y = 'b'
      >
      > [well.A3]
      > x = 'a'
      > y = 'c'
    expected: nan_first.png
  -
    id: nan-in-other-param
    layout:
      > [well]
      > A1.x = 1
      > A2.y = 2
      > A3.x = 3
      > A3.y = 4
    expected: nan_in_other_param.png
  -
    id: skip-wells
    layout:
      > [well.B2]
      > x = 'a'
      >
      > [well.D4]
      > x = 'b'
    expected: skip_wells.png
  -
    id: long-labels
    layout:
      > [well]
      > A1.x = 'long string'
      > A2.x = 'longer string'
