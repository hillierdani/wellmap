test_show:
  -
    id: no-wells
    layout:
    error:
      type: LayoutError
      message: No wells defined
  -
    id: no-params
    layout:
      > [well.A1]
      >
      > [well.A2]
    error:
      type: LayoutError
      message: No experimental parameters found
  -
    id: degenerate-param
    layout:
      > [well.A1]
      > x = 1
    error:
      type: UsageError
      message:
        - Found only degenerate parameters
        - 'x'
  -
    id: unknown-param
    layout:
      > [well]
      > A1.x = 1
      > A2.x = 2
    params: XXX
    error:
      type: UsageError
      message:
        - No such parameter: 'XXX'
        - Did you mean: 'x'
  -
    id: one-param
    layout:
      > [well]
      > A1.x = 1
      > A2.x = 2
    expected: one_param.png
  -
    id: two-params
    layout:
      > [well.A1]
      > x = 1
      > y = 'a'
      >
      > [well.A2]
      > x = 2
      > y = 'b'
    expected: two_params.png
  -
    id: user-params-x-1
    layout:
      > [well.A1]
      > x = 1
    params: x
    expected: user_params_x_1.png
  -
    id: user-params-x-2
    layout:
      > [well.A1]
      > x = 1
      > y = 'a'
      >
      > [well.A2]
      > x = 2
      > y = 'b'
    params: x
    expected: user_params_x_2.png
  -
